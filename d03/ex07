#include<stdio.h>

#include<string.h>

char *ft_strrev(char *str)
{
	int a;
	int b;
	char c;

	a = 0;
	b = 0;
	while (str[a] != '\0')
	{
		a++;
	}
	a--;
	{
		while
	}
}
